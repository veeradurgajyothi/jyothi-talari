l=[]
n=int(input('enter n'))
for i in range(n):
    x=int(input())
    l.append(x)
    min=l[0]
    max=l[0]
    for i in range(1,n):
        if l[i]<min:
            min=l[i]
            print('minimum value:',min)
            for i in range(1,n):
                if l[i]>max:
                    max=l[i]
    print('maximum value:',max)
                
                
               
               
                    
                
            
    
